<mapping>
	<class name="nl.dalthow.etaron.framework.KeyMap">
		<map-to xml="key-map"/>
		
		<field name="jump" type="integer">
			<bind-xml name="jump" node="attribute"/>
		</field>
		
		<field name="moveRight" type="integer">
			<bind-xml name="moveRight" node="attribute"/>
		</field>
		
		<field name="moveLeft" type="integer">
			<bind-xml name="moveLeft" node="attribute"/>
		</field>
		
		<field name="tradeItem" type="integer">
			<bind-xml name="tradeItem" node="attribute"/>
		</field>
		
		<field name="dropItem" type="integer">
			<bind-xml name="dropItem" node="attribute"/>
		</field>
		
		<field name="switchPlayer" type="integer">
			<bind-xml name="switchPlayer" node="attribute"/>
		</field>
		
		<field name="backKey" type="integer">
			<bind-xml name="backKey" node="attribute"/>
		</field>
		
		<field name="performanceInfo" type="integer">
			<bind-xml name="performanceInfo" node="attribute"/>
		</field>
	</class>
</mapping>