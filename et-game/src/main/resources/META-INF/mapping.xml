<mapping>
	<class name="nl.dalthow.etaron.framework.KeyMap">
		<map-to xml="key-map"/>
		<field name="jump" type="string">
			<bind-xml name="jump" node="string"/>
		</field>
		
		<field name="switch-player" type="string">
			<bind-xml name="switch-player" node="string"/>
		</field>
		
		<field name="back-key" type="string">
			<bind-xml name="back-key" node="string"/>
		</field>
		
		<field name="trade-item" type="string">
			<bind-xml name="trade-item" node="string"/>
		</field>
		
		<field name="move-right" type="string">
			<bind-xml name="move-right" node="string"/>
		</field>
		
		<field name="move-left" type="string">
			<bind-xml name="move-left" node="string"/>
		</field>
		
		<field name="performance-info" type="string">
			<bind-xml name="performance-info" node="string"/>
		</field>
	</class>
</mapping>